{
  "expo": {
    "name": "furo-expo-example",
    "slug": "furo-expo-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/furo-logo.png",
    "userInterfaceStyle": "light",
    "scheme": "exp",
    "splash": {
      "image": "./assets/furo-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "one.furo.expo-example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/furo-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/furo-logo.png"
    }
  }
}
